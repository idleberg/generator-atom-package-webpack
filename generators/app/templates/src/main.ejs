import { CompositeDisposable } from 'atom';

export default {
<%= pkg.ws %>config: {},
<%= pkg.ws %>subscriptions: null,

<%= pkg.ws %>activate() {

<%= pkg.ws %><%= pkg.ws %>// Events subscribed to in atom's system can be easily cleaned up with a CompositeDisposable
<%= pkg.ws %><%= pkg.ws %>this.subscriptions = new CompositeDisposable;

<%= pkg.ws %><%= pkg.ws %>// Register commands
<%= pkg.ws %><%= pkg.ws %>this.subscriptions.add(atom.commands.add('atom-workspace', {
<%= pkg.ws %><%= pkg.ws %><%= pkg.ws %>'<%= pkg.name %>:hello-world': () => this.helloWorld()
<%= pkg.ws %><%= pkg.ws %>}));
<%= pkg.ws %>},

<%= pkg.ws %>deactivate() {
<%= pkg.ws %><%= pkg.ws %>this.subscriptions.dispose();
<%= pkg.ws %>},

<%= pkg.ws %>helloWorld() {
<%= pkg.ws %><%= pkg.ws %>atom.notifications.addInfo('Hello World');
<%= pkg.ws %>}
};
